
#ifndef WORK_H_
#define WORK_H_

#include <tque>
#include <memory>

class Work
{
public:
	Work() {}
	virtual ~Work() {}

	virtual bool begin() = 0;
	virtual void run() = 0;
	virtual void end() = 0;
};

typedef std::shared_ptr<Work> WorkPtr;
typedef TQue<Work> WorkQue;
	
#endif //  WORK_H_ 
